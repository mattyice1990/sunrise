{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/about",
      "destination": "/about-our-roofing-contractors-in-tucson",
      "permanent": true
    },
    {
      "source": "/why-choose-sunrise-roofers",
      "destination": "/why-choose-sunrise-roofers-in-tucson",
      "permanent": true
    },
    {
      "source": "/services",
      "destination": "/roofing-services",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

